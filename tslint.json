{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "ordered-imports": false,
    "semicolon": false,
    "no-console": false,
    "arrow-parens": [true, "ban-single-arg-parens"],
    "member-access": false,
    "object-literal-sort-keys": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ],
    "no-var-requires": false,
    "align": false,
    "no-empty": false,
    "whitespace": false
  },
  "rulesDirectory": []
}
