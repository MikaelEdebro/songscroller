{
  "name": "songscroller",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "10.9.0",
    "npm": "6.2.0"
  },
  "scripts": {
    "start": "ts-node server/src/index.js",
    "dev": "concurrently \"nodemon\" \"npm start --prefix client\"",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "t": "jest --watch --detectOpenHandles",
    "test": "jest --runInBand"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/client/", "/dist/"],
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testEnvironment": "node",
    "verbose": false,
    "testMatch": ["**/*.(test|spec).(js|ts|tsx)"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikaelEdebro/songscroller.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MikaelEdebro/songscroller/issues"
  },
  "homepage": "https://github.com/MikaelEdebro/songscroller#readme",
  "dependencies": {
    "await-to-js": "^2.0.1",
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-mongo-sanitize": "^1.3.2",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "mongoose": "^5.1.7",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cookie-session": "^2.0.35",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/express-mongo-sanitize": "^1.3.1",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^23.3.1",
    "@types/mongo-sanitize": "^1.0.0",
    "@types/mongoose": "^5.2.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.9.3",
    "@types/passport": "^0.4.6",
    "@types/passport-facebook": "^2.1.8",
    "@types/supertest": "^2.0.5",
    "concurrently": "^3.6.0",
    "jest": "^23.5.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  }
}
