{
  "name": "songscroller",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "9.5.0",
    "npm": "5.7.1"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon index.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild":
      "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "t": "jest --watchAll --runInBand",
    "test": "jest --runInBand"
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "/client/"],
    "testEnvironment": "node",
    "verbose": false,
    "setupTestFrameworkScriptFile": "./test/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MikaelEdebro/songscroller.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MikaelEdebro/songscroller/issues"
  },
  "homepage": "https://github.com/MikaelEdebro/songscroller#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "helmet": "^3.12.1",
    "mongo-sanitize": "^1.0.0",
    "mongoose": "^5.1.7",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.0",
    "jest": "^23.5.0",
    "nodemon": "^1.17.5",
    "supertest": "^3.1.0"
  }
}
